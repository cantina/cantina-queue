{
  "env": {
    "browser": true,
    "node": true,
    "amd": true
  },
  "extends": "eslint-config-airbnb-es5",
  "rules": {
    "brace-style": [2, "stroustrup"],
    "camelcase": 0,
    "comma-style": 0,
    "consistent-return": 0,
    "func-names": 0,
    "indent": [2, 2, {"SwitchCase": 1}],
    "no-else-return": 0,
    "no-param-reassign": 0,
    "no-shadow": [1, {"allow": ["err", "cb", "done", "next"]}],
    "one-var": 0,
    "padded-blocks": 0,
    "space-before-function-paren": [1, "always"]
  }
}